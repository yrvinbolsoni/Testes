@model IEnumerable<Testes.Models.D>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.data)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fileName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fileExtension)
        </th>
        <th></th>
    </tr>
 

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.fileName , "MostarArquivoDobanco", new { id = item.Id })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fileName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fileExtension)
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.Id }) |
                @Html.ActionLink("Donwload", "DonwloadArquivoDobanco", new { id = item.Id }) |
                @Html.ActionLink("Mostar Img View", "MostarImgNaview", new { id = item.Id })
            </td>
        </tr>
    }


</table>
